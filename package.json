{
  "name": "knex",
  "version": "0.7.3",
  "description": "A batteries-included SQL query & schema builder for Postgres, MySQL and SQLite3 and the Browser",
  "main": "knex.js",
  "directories": {
    "test": "test"
  },
  "browser": {
    "./src/dialects/oracle": false,
    "./src/dialects/strong-oracle": false,
    "./src/dialects/maria": false,
    "./src/dialects/mysql": false,
    "./src/dialects/mysql2": false,
    "./src/dialects/postgres": false,
    "./src/dialects/sqlite3": false,
    "sqlite3": false
  },
  "dependencies": {
    "bluebird": "^2.0.0",
    "chalk": "~0.5.1",
    "commander": "^2.2.0",
    "inherits": "~2.0.1",
    "interpret": "^0.3.2",
    "liftoff": "~2.0.0",
    "lodash": "^3.5.0",
    "minimist": "~1.1.0",
    "mkdirp": "^0.5.0",
    "pool2": "^1.0.0",
    "readable-stream": "^1.1.12",
    "tildify": "~1.0.0",
    "duce": "0.1.0",
    "v8flags": "^2.0.2",
    "babel-runtime": "^5.0.12"
  },
  "devDependencies": {
    "babel": "^5.0.12",
    "babel-loader": "^5.0.0",
    "browserify": "~5.12.1",
    "chai": "^1.9.1",
    "chai-as-promised": "^4.1.0",
    "coveralls": "~2.11.1",
    "gulp": "^3.6.2",
    "gulp-bump": "^0.1.8",
    "gulp-git": "~0.5.0",
    "gulp-jshint": "^1.8.4",
    "gulp-shell": "^0.2.5",
    "istanbul": "~0.3.0",
    "jshint": "~2.5.1",
    "mariasql": "~0.1.21",
    "mocha": "^1.20.0",
    "mysql": "^2.3.1",
    "mysql2": "~0.12.1",
    "node-uuid": "~1.4.0",
    "pg": "^3.2.0",
    "pg-query-stream": "^0.6.0",
    "rimraf": "2.x",
    "sinon": "^1.10.0",
    "sinon-chai": "^2.5.0",
    "sqlite3": "~3.0.2",
    "through": "^2.3.4"
  },
  "scripts": {
    "standalone": "webpack -w",
    "build": "rm -rf ./lib && babel src--loose=all --optional=runtime --copy-files --remove-comments --out-dir lib --source-maps",
    "dev": "babel src --watch --loose=all --optional=runtime --copy-files --remove-comments --out-dir lib --source-maps",
    "plaintest": "mocha --compilers js:babel/register --check-leaks -t 5000 -b -R spec test/index.js",
    "test": "npm run jshint && istanbul --config=test/.istanbul.yml cover _mocha -- --check-leaks -t 5000 -b -R spec test/index.js",
    "coveralls": "cat ./test/coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "jshint": "gulp jshint",
    "release:patch": "gulp jshint && npm test && gulp build && gulp bump --type patch && gulp release",
    "release:minor": "gulp jshint && npm test && gulp build && gulp bump --type minor && gulp release"
  },
  "bin": {
    "knex": "./lib/bin/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/tgriesser/knex.git"
  },
  "keywords": [
    "sql",
    "query",
    "postgresql",
    "mysql",
    "sqlite3"
  ],
  "author": {
    "name": "Tim Griesser",
    "web": "https://github.com/tgriesser"
  },
  "files": [
    "README.md",
    "lib/*",
    "browser/*",
    "gulpfile.js",
    "knex.js",
    "LICENSE"
  ],
  "license": "MIT"
}
